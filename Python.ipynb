        "from collections import Counter\n",
        "a=input()\n",
        "b=a.split()\n",
        "Counter=Counter(b)\n",
        "c=Counter.most_common(2)\n",
        "d=[]\n",
        "e=b[::-1]\n",
        "for i in c :\n",
        "    for j in i:\n",
        "      d.append(j)\n",
        "print(' '.join(e))\n",
        "print(d[0],':',d[1],\"times\")\n",
        "print(d[2],':',d[3],\"times\")"
